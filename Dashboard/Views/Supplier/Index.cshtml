<div class="table">

    <table id="myTable" class="table table-bordered table-hover table-sm">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>DOB</th>
                <th>Email</th>
                <th>Ph:No</th>
                <th>Gender</th>
                <th>Occupation</th>
                <th>ID Type</th>
                <th>ID No:</th>
                <th>Issued Authority</th>
                <th>Issued State</th>
                <th>Issued Date</th>
                <th>Exp Date</th>
                <th>Address</th>
                <th>Nationality</th>
