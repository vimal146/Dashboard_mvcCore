                    <td>@addsupplier.IDType</td>
                    <td>@addsupplier.IDNumber</td>
                    <td>@addsupplier.IssueAuthority</td>
                    <td>@addsupplier.IssuedState</td>
                    <td>@addsupplier.IssuedDate</td>
                    <td>@addsupplier.ExpiryDate</td>
                    <td>@addsupplier.AddressType</td>
                    <td>@addsupplier.Nationality</td>
                    <td>@addsupplier.District</td>
                    <td>@addsupplier.State</td>
                    <td>@addsupplier.BlockNumber</td>
                    <td>@addsupplier.Pincode</td>
                    <td>@addsupplier.GST</td>
                    <td>@addsupplier.PanNo</td>
                    <td>@addsupplier.OpeningBalance</td>
                    <td>@addsupplier.BankName</td>
                    <td>@addsupplier.BankAccNo</td>
                    <td>@addsupplier.IFSCCode</td>
                    <td><a href="/Supplier/View/@addsupplier.Id">View</a></td>

